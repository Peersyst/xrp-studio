{
    "name": "xrp-studio",
    "private": true,
    "scripts": {
        "start": "yarn docker:start:dev && lerna run start --stream",
        "start:e2e": "yarn docker:clean:e2e && yarn docker:start:e2e && lerna run --scope xrp-studio-backend db:pack && lerna run start:e2e --stream",
        "bootstrap": "lerna bootstrap",
        "build": "lerna run build --stream",
        "clean": "lerna clean",
        "docker:start:dev": "docker-compose up -d",
        "docker:start:e2e": "docker-compose -f ./docker/config/docker-compose-e2e.yml --env-file .env up -d"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.10.0",
        "cypress": "^10.9.0",
        "cypress-react-selector": "^3.0.0",
        "lerna": "5.1.6",
        "rimraf": "^3.0.2",
        "start-server-and-test": "^1.14.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {}
}
