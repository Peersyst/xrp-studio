{
    "name": "xrp-studio-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prebuild": "rimraf build",
        "start": "npm run generate && craco start",
        "build": "craco build",
        "eject": "craco eject",
        "generate": "npm run generate:openapi && npm run generate:icons && npm run generate:images",
        "generate:icons": "node ./src/script/iconGenerator.js ./src/asset/icon/ ./src/module/common/icons/",
        "generate:images": "node ./src/script/imagesGenerator.js ./src/asset/image/",
        "generate:openapi": "openapi -i ../backend/openapi-spec.json -o src/module/api/service --exportSchemas=true --useUnionTypes",
        "test": "jest --maxWorkers=30% --coverage",
        "test:only": "jest --maxWorkers=30% --coverage --",
        "test:unit": "jest --maxWorkers=30% --coverage ./test",
        "test:unit:ci": "jest --runInBand --coverage ./test",
        "test:debug": "jest --coverage --no-cache --runInBand || true",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json --max-warnings 0"
    },
    "dependencies": {
        "@craco/craco": "^6.4.4",
        "@peersyst/react-components": "^3.0.3",
        "@peersyst/react-hooks": "^2.0.1",
        "@peersyst/react-types": "^1.5.0",
        "@peersyst/react-utils": "^2.0.0",
        "babel-plugin-styled-components": "^2.0.7",
        "color": "^4.2.3",
        "craco-alias": "^3.0.1",
        "i18next": "^21.8.11",
        "i18next-browser-languagedetector": "^6.1.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.17.4",
        "react-query": "^3.34.12",
        "react-router-dom": "6.3.0",
        "react-scripts": "^4.0.0",
        "recoil": "^0.7.4",
        "styled-components": "^5.3.5",
        "typescript": "^4.7.4",
        "web-vitals": "^1.0.1",
        "xrpl": "^2.5.0",
        "xumm-react": "^1.0.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.18.6",
        "@babel/plugin-transform-runtime": "7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@cypress/code-coverage": "3.10.0",
        "@cypress/instrument-cra": "^1.4.0",
        "@testing-library/cypress": "8.0.3",
        "@testing-library/dom": "8.11.1",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.0",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/color": "^3.0.3",
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.3.2",
        "@types/styled-components": "^5.1.20",
        "@typescript-eslint/eslint-plugin": "^5.30.4",
        "@typescript-eslint/parser": "^5.30.4",
        "babel-jest": "^26.6.3",
        "craco-alias": "^3.0.1",
        "customize-cra": "^1.0.0",
        "customize-cra-react-refresh": "^1.1.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "26.6.0",
        "openapi-typescript": "^5.4.0",
        "openapi-typescript-codegen": "^0.23.0",
        "react-error-overlay": "6.0.9",
        "ts-jest": "^26.5.6"
    },
    "resolutions": {
        "@types/react": "17.0.2",
        "@types/react-dom": "17.0.2",
        "react-error-overlay": "6.0.9"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
